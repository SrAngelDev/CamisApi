# Schema GraphQL simple para Carrito (DAW) - Solo consultas

type Carrito {
    id: ID!
    userId: ID!
    items: [String!]!
    totalItems: Int!
    modificadoEn: String!
    creadoEn: String!
    isValid: Boolean!
}

type Query {
    carritos: [Carrito!]!
    carrito(id: ID!): Carrito
    carritoByUserId(userId: ID!): Carrito
}

